{
  "name": "yuitest-bot",
  "version": "0.0.0",
  "license": "MIT",
  "author": "yuitest <yuitest@cjhat.net>",
  "description": "yuitest-bot",
  "scripts": {
    "build": "tsup src/*.ts --format cjs,esm --minify --clean --sourcemap",
    "dev": "pnpm run build --watch",
    "run:update": "tsx src/update.ts",
    "run:random": "tsx src/random.ts",
    "run:keep-alive": "tsx src/keep-alive.ts",
    "run:dailyHabit": "tsx src/dailyHabit.ts",
    "test": "jest --coverage",
    "lint": "biome check .",
    "format": "biome check --write ."
  },
  "bin": {
    "random": "./dist/random.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "package.json"],
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@swc/core": "1.7.35",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/luxon": "^3.4.2",
    "@types/twitter": "^1.7.4",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "json5": "^2.2.3",
    "luxon": "^3.5.0",
    "tsup": "^8.3.0",
    "twitter-api-v2": "^1.18.0"
  },
  "packageManager": "pnpm@9.4.0"
}
