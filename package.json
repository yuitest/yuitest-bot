{
  "name": "yuitest-bot",
  "version": "0.0.0",
  "license": "MIT",
  "author": "yuitest <yuitest@cjhat.net>",
  "description": "yuitest-bot",
  "scripts": {
    "build": "tsup src/*.ts --format cjs,esm --minify --clean --sourcemap",
    "dev": "pnpm run build --watch",
    "run:update": "tsx src/update.ts",
    "run:random": "tsx src/random.ts",
    "run:keep-alive": "tsx src/keep-alive.ts",
    "run:dailyHabit": "tsx src/dailyHabit.ts",
    "test": "jest --coverage",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write ."
  },
  "bin": {
    "random": "./dist/random.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "@swc/core": "^1.3.99",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.10",
    "@types/luxon": "^3.3.5",
    "@types/twitter": "^1.7.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.461.0",
    "json5": "^2.2.3",
    "luxon": "^3.4.4",
    "tsup": "^8.0.1",
    "twitter-api-v2": "^1.15.2"
  }
}
